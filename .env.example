# Environment Configuration
# Copy this file to .env: cp .env.example .env

# Ollama (Local LLM - no API key required)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1

# LangSmith (Optional - for debugging/tracing)
# Sign up at https://smith.langchain.com/ for free API key
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=procurement-agent
LANGSMITH_TRACING=false

# Application
DATA_DIR=Agents - Code Challenge/Data
LOG_LEVEL=INFO
